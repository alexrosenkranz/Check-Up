{
  "name": "check-up",
  "version": "1.0.0",
  "description": "a patient healthcare monitoring app",
  "main": "server/server.js",
  "scripts": {
    "dev": "export NODE_ENV=\"development\"; nodemon server/server.js",
    "dev-migrate:all": "export NODE_ENV=\"development\"; ./node_modules/.bin/sequelize db:migrate",
    "dev-migrate:undo:all": "export NODE_ENV=\"development\"; ./node_modules/.bin/sequelize db:migrate:undo:all",
    "test-migrate:all": "export NODE_ENV=\"testing\"; ./node_modules/.bin/sequelize db:migrate",
    "test-migrate:undo:all": "export NODE_ENV=\"testing\"; ./node_modules/.bin/sequelize db:migrate:undo:all",
    "test": "export NODE_ENV='testing'; export PORT='1010'; find ./test -name '*.js' | node_modules/.bin/mocha --recursive",
    "lint:server": "node_modules/.bin/eslint --fix server/",
    "lint:test": "node_modules/.bin/eslint --fix test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexrosenkranz/Check-Up.git"
  },
  "author": "Alan Chu, Alex Rosenkranz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexrosenkranz/Check-Up/issues"
  },
  "homepage": "https://github.com/alexrosenkranz/Check-Up#readme",
  "dependencies": {
    "axios": "^0.16.0",
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "mysql": "^2.13.0",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.2.0",
    "morgan": "^1.8.1"
  }
}
